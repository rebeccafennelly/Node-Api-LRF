{
  "name": "lrf-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all build serve",
    "serve": "node ./dist/index",
    "watch": "nodemon",
    "clean": "rimraf dist",
    "transpile": "babel ./src --out-dir dist",
    "build": "npm-run-all clean transpile",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "body-parser": "1.9.x",
    "express": "~4.17.3",
    "firebase-admin": "^9.0.0",
    "minimist": "*",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "swagger-node-express": "~2.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "jest": "^25.4.0"
  },
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "10"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": "v12.18.1"
  }
}
